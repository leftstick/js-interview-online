{
  "name": "js-interview-online",
  "version": "1.0.0",
  "description": "Interview tool for javascript",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "prepare": "umi generate tmp && husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{j,t}s?(x)": "eslint --max-warnings=0 --ignore-pattern=public/**/*"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "ahooks": "^2.10.12",
    "assert": "^2.0.0",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "peerjs": "^1.3.2",
    "prism-react-renderer": "^1.2.1",
    "re-resizable": "^6.9.1",
    "react-ace": "^9.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/classnames": "^2.3.1",
    "@types/lodash": "^4.14.177",
    "@umijs/plugin-antd": "^0.12.1",
    "@umijs/plugin-locale": "^0.15.1",
    "@umijs/plugin-model": "^2.6.2",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "react-coding-style": "^1.3.1",
    "umi": "^3.5.20"
  }
}
